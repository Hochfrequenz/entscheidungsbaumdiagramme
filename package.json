{
  "name": "ebd.hochfrequenz.de",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.19 <21"
  },
  "scripts": {
    "start": "vite dev --open",
    "dev": "vite dev",
    "build": "node src/lib/data/retired-ebds.js && vite build",
    "serve": "vite build && npx http-server ./build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier '**/*.{svelte,js,jsx,ts,tsx,html,css,scss,json,yml,md}' --write",
    "format:check": "prettier '**/*.{svelte,js,jsx,ts,tsx,html,css,scss,json,yml,md}' --check",
    "lint": "eslint 'src/**/*.{js,ts,svelte}' --fix",
    "lint:check": "eslint 'src/**/*.{js,ts,svelte}'"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-svelte": "^3.13.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.93.3",
    "scss": "^0.2.4",
    "svelte-check": "^4.3.3",
    "svelte": "^5.43.5",
    "tailwindcss": "^3.4.18",
    "tslib": "^2.8.0",
    "vite": "^7.1.10",
    "typescript": "^5.9.3"
  },
  "type": "module",
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.8.0"
  }
}
