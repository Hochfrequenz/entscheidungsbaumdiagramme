    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.89.2",
    "scss": "^0.2.4",
    "svelte-check": "^4.3.1",
    "svelte": "^5.37.3",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  },
  "type": "module",
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.2.0"
