{
  "name": "ebd.hochfrequenz.de",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite dev --open",
    "dev": "vite dev",
    "build": "vite build",
    "serve": "vite build && npx http-server ./build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier '**/*.{svelte,js,jsx,ts,tsx,html,css,scss,json,yml,md}' --write",
    "format:check": "prettier '**/*.{svelte,js,jsx,ts,tsx,html,css,scss,json,yml,md}' --check",
    "lint": "eslint 'src/**/*.{js,ts,svelte}' --fix",
    "lint:check": "eslint 'src/**/*.{js,ts,svelte}'",
    "postinstall": "npx playwright install",
    "test": "npx playwright test"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/node": "^22.9.3",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.11.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-svelte": "^2.46.1",
    "lucide-svelte": "^0.456.0",
    "playwright": "^1.48.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.3.2",
    "sass": "^1.80.6",
    "scss": "^0.2.4",
    "svelte": "^5.2.2",
    "svelte-check": "^4.1.0",
    "tailwindcss": "^3.4.15",
    "tslib": "^2.8.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.9"
  },
  "type": "module",
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.3"
  }
}
