  "private": true,
  "scripts": {
    "start": "vite dev --open",
    "dev": "vite dev",
    "build": "vite build",
    "serve": "vite build && npx http-server ./build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier '**/*.{svelte,js,jsx,ts,tsx,html,css,scss,json,yml,md}' --write",
    "format:check": "prettier '**/*.{svelte,js,jsx,ts,tsx,html,css,scss,json,yml,md}' --check",
    "lint": "eslint 'src/**/*.{js,ts,svelte}' --fix",
    "lint:check": "eslint 'src/**/*.{js,ts,svelte}'"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.4",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.29.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-svelte": "^3.4.0",
    "prettier": "^3.5.3",
    "postcss": "^8.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "sass": "^1.86.0",
    "scss": "^0.2.4",
    "svelte": "^5.25.3",
    "svelte-check": "^4.1.5",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.0",
    "typescript": "^5.8.3",
    "vite": "^5.4.17"
  },
  "type": "module",
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.3"
  }
