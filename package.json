{
  "name": "ebd.hochfrequenz.de",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite dev --open",
    "dev": "vite dev",
    "build": "vite build",
    "serve": "vite build && npx http-server ./build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier '**/*.{svelte,js,jsx,ts,tsx,html,css,scss,json,yml,md}' --write",
    "format:check": "prettier '**/*.{svelte,js,jsx,ts,tsx,html,css,scss,json,yml,md}' --check",
    "lint": "eslint 'src/**/*.{js,ts,svelte}' --fix",
    "lint:check": "eslint 'src/**/*.{js,ts,svelte}'",
    "postinstall": "npx playwright install",
    "test": "npx playwright test"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.15.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@types/node": "^22.13.0",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.25.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-svelte": "^2.46.1",
    "playwright": "^1.49.1",
    "prettier": "^3.5.3",
    "postcss": "^8.5.2",
    "prettier-plugin-svelte": "^3.3.3",
    "sass": "^1.85.1",
    "scss": "^0.2.4",
    "svelte": "^5.20.2",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.0",
    "typescript": "^5.7.3",
    "vite": "^5.4.9"
  },
  "type": "module",
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.3"
  }
}
